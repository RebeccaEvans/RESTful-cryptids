<h1>EDIT A CRYPTID</h1>

<form method="POST" action="/cryptids/<%=cryptidId%>/?_method=PUT">
    <label>Name</label>
    <input type="text" name="name" value="<%= cryptid.name %>">
    <label>Type</label>
    <input type="text" name="type" value="<%= cryptid.type %>">
    <input type="submit">
</form>